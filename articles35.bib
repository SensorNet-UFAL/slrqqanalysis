@inproceedings{ ISI:000234856100050,
Author = {Bakshi, A and Prasanna, VK},
Editor = {{Pal, A and Kshemkalyani, AD and Kumar, R and Gupta, A}},
Title = {{Programming paradigms for networked sensing: A distributed systems'
   perspective}},
